{
	"manifest_version": 2,
	"name": "Translate this",
	"description": "Translate words or phrases in page",
	"version": "0.2",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	],
	"background": {
		"scripts": ["js/event.js"],
		"persistent": false
	}
}